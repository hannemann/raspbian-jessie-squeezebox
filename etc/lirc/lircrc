	prog = irexec
	remote = remote
	button = vga
	config = /bin/su -c "/home/pi/bin/keys-lib grave" pi
end
##### Library #####
begin
	prog = irexec
	remote = remote
	button = s-video
	config = /bin/su -c "/home/pi/bin/keys-lib semicolon" pi
end
##### Track Info #####
begin
	prog = irexec
	remote = remote
