#################
#
# Navigation
#
#################
begin
	prog = irexec
	remote = remote
	button = up
	delay = 5
	repeat = 2
	config = /bin/su -c"/home/pi/bin/keys-lib up" pi
end
begin
	prog = irexec
	remote = remote
	button = down
	delay = 5
	repeat = 2
	config = /bin/su -c "/home/pi/bin/keys-lib down" pi
end
begin
	prog = irexec
	remote = remote
	button = left
	delay = 5
	repeat = 2
	config = /bin/su -c "/home/pi/bin/keys-lib left" pi
end
begin
	prog = irexec
	remote = remote
	button = right
	delay = 5
	repeat = 2
	config = /bin/su -c "/home/pi/bin/keys-lib right" pi
end
begin
	prog = irexec
	remote = remote
	button = select
	config = /bin/su -c "/home/pi/bin/keys-lib ok" pi
end
begin
	prog = irexec
	remote = remote
	button = component
	config = /bin/su -c "/home/pi/bin/keys-lib esc" pi
end
#####################
#
# Power
#
#####################
begin
	prog = irexec
	remote = remote
	button = power_toggle
	config = /bin/su -c "/home/pi/bin/toggle-psu" pi
end
#####################
#
# Volume
#
#####################
begin
	prog = irexec
	remote = remote
	button = menu
	repeat = 2
	config = /bin/su -c '/home/pi/bin/keys-lib +' pi
end
begin
	prog = irexec
	remote = remote
	button = return
	repeat = 2
	config = /bin/su -c '/home/pi/bin/keys-lib -' pi
end
#####################
#
# Playback control
#
#####################
#### Play #####
begin
	prog = irexec
	remote = remote
	button = play
	config = /bin/su -c '/home/pi/bin/keys-lib p' pi
end
#### Stop #####
begin
	prog = irexec
	remote = remote
	button = stop
	config = /bin/su -c '/home/pi/bin/keys-lib g' pi
end
#### Play/Pause #####
begin
	prog = irexec
	remote = remote
	button = pause
	config = /bin/su -c '/home/pi/bin/keys-lib space' pi
end
#### Skip FWD #####
begin
	prog = irexec
	remote = remote
	button = skip_fwd
	config = /bin/su -c '/home/pi/bin/keys-lib b' pi
end
#### Skip back #####
begin
	prog = irexec
	remote = remote
	button = skip_back
	config = /bin/su -c '/home/pi/bin/keys-lib z' pi
end
#### Toggle shuffle #####
begin
	prog = irexec
	remote = remote
	button = setup
	config = /bin/su -c '/home/pi/bin/keys-lib comma' pi
end
#### Toggle repeat #####
begin
	prog = irexec
	remote = remote
	button = repeat
	config = /bin/su -c '/home/pi/bin/keys-lib dot' pi
end
#### search fwd #####
begin
	prog = irexec
	remote = remote
	button = ffwd
	repeat = 2
	config = /bin/su -c '/home/pi/bin/keys-lib r' pi
end
#### search bwd #####
begin
	prog = irexec
	remote = remote
	button = rew
	repeat = 2
	config = /bin/su -c '/home/pi/bin/keys-lib e' pi
end
#### add at end #####
begin
	prog = irexec
	remote = remote
	button = 10+
	config = /bin/su -c '/home/pi/bin/keys-lib d' pi
end
#### play next #####
begin
	prog = irexec
	remote = remote
	button = 3d
	config = /bin/su -c '/home/pi/bin/keys-lib y' pi
end
#### create mix #####
begin
	prog = irexec
	remote = remote
	button = zoom
	config = /bin/su -c '/home/pi/bin/keys-lib m' pi
end
